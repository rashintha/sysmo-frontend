{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": [2, "never"],
        "object-curly-spacing": [2, "always"],
        "indent": ["warn", 2],
        "react/react-in-jsx-scope": "off",
        "max-len": ["warn", 140]
    }
}
